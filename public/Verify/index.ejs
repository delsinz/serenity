<!DOCTYPE html>
<html lang="en">
<head>
  <title>Username's Profile</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
<link rel="stylesheet" type="text/css" href="stylesheet.css">
  <script src="https://code.jquery.com/jquery-1.12.1.min.js"></script>
  <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
  <link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.4.0/css/font-awesome.min.css">
</head>
<body>
<!-- start navigation bar-->
  <nav class="navbar navbar-inverse">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href="">
          <img src = <%= url+"logo.png" %> alt = "Serenity" class="img-rounded" style = "width:50px;height:50px;">
        </a>
      </div>
      <div class="collapse navbar-collapse" id="myNavbar">
        <ul class="nav navbar-nav">
          <li class="active"><a href="#">Serenity</a></li>
        </ul>
        <ul class="nav navbar-nav navbar-right">
          <li class="logout"><a href="#"><i class="fa fa-power-off fa-lg"></i> Log Out</a></li>
        </ul>
      </div>
    </div>
  </nav>
<!-- end navigation bar-->

    <!-- start posting status box-->
    <div class="row username" id="<%=username%>">
      <div class="col-sm-12">
      <div class="panel panel-default text-left">
        <div class="panel-body">
          <h3>Please Verify Your Email</h3>
          <button class="verify-button btn btn-default"></i>
              </span> Resend Verification Link
            </button>
        </div>
      </div>
    </div>
  </div>
  <!-- end posting status box-->
    <!-- start activity feed-->
    <div class="panel-group text-left feed">
  </div>
<!-- end middle panel-->

<!-- footer -->
<footer>
  <div class="row">
    <div class="col-sm-12"
      <a href=#>Cool Doves</a> All rights reserved.
    </div>
  </div>
</footer>
<script src="master.js"></script>
<!-- end footer -->

</body>
</html>
